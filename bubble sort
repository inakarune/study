var arr = [2, 1, 9, 3, 3, 5, 8, 0, 1, 4];
    var j = 0;
    var count = 0;
    while (count <= arr.length){
      for (var i = 0; i < arr.length; i++){
          if (arr[i] > arr[i + 1]){
            j = arr[i + 1];
            arr[i + 1] = arr[i];
            arr[i] = j;
            document.write(arr + '<br>')
          }
      }
      count++
    }
